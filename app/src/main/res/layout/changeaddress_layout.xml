<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:id="@+id/setToDefaultLayout"
    >

    <TextView
        android:id="@+id/txtviewUserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:gravity="start"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textFontWeight="600"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="UserName" />


    <TextView
        android:id="@+id/txtviewshortAddress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textFontWeight="500"
        android:textSize="14sp"
        android:maxLines="1"
        android:ellipsize="end"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txtviewUserName"
        app:layout_constraintTop_toBottomOf="@+id/txtviewUserName"
        tools:text="UserName" />

    <ImageView
        android:id="@+id/imgviewdefault"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginStart="16dp"
        android:contentDescription="Default"
        app:layout_constraintBottom_toBottomOf="@+id/txtviewUserName"
        app:layout_constraintStart_toEndOf="@+id/txtviewUserName"
        app:layout_constraintTop_toTopOf="@+id/txtviewUserName"
        app:srcCompat="@drawable/select26" />

    <ImageView
        android:id="@+id/imgviewmnu"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/txtviewshortAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dots" />


</androidx.constraintlayout.widget.ConstraintLayout>